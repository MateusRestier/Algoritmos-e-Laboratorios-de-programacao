// Seção de Declarações 
var
   raio: real
   base, contador, resultado: real
   expoente: inteiro
   area: real
   
inicio
escreval("Informe o valor do raio")
leia(raio)
area <- 4 * pi * raio^2
escreval("O valor do volume da esfera é de: ",area:6:2, " m3")




 
fimalgoritmo
