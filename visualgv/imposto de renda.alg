algoritmo "semnome"
// Função :
// Autor :
// Data : 08/09/2021
// Seção de Declarações 
var

salario: real


inicio
escreva("Digite o salário: ")
leia(salario)

se salario <= 1850 entao
escreval ("salário isento do imposto de renda")
senao
escreval ("salário não isento do imposto de renda")
fimse





fimalgoritmo
